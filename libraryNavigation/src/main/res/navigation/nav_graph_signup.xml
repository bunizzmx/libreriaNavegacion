<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_signup"
    app:startDestination="@id/singUpFragment">

    <fragment
        android:id="@+id/singUpFragment"
        android:name="com.example.featuresigup.ui.SingUpFragment"
        android:label="SingUpFragment"
        tools:layout="@layout/fragment_sing_up">

        <action
            android:id="@+id/go_to_webview"
            app:destination="@id/nav_graph_webview" />
        <action
            android:id="@+id/go_to_create_pass"
            app:destination="@id/nav_graph_create_pass" />
        <action
            android:id="@+id/go_to_login_pass"
            app:destination="@id/nav_graph_pass" />

    </fragment>
    <include app:graph="@navigation/nav_graph_webview" />
    <include app:graph="@navigation/nav_graph_create_pass" />
    <include app:graph="@navigation/nav_graph_pass" />

</navigation>